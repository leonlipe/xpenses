


<div class="row">
<div class="col-12">
<div class="widget-box">
	<div class="widget-title">
		<span class="icon">
			<i class="glyphicon glyphicon-th"></i>
		</span>
		<h5>Dynamic table</h5>
	</div>
	<div class="widget-content nopadding">

	<table class="table table-striped table-bordered data-table table-hover" id="movements_datatable" data-source="<%= accounts_url(format: "json") %>">
		<thead>
	  <tr>
	    <th>ID</th>
	    <th>Fecha</th>
	    <th>Descripcion</th>
	    <th>Cantidad</th>
	    <th>Tipo</th>
	  </tr>
	</thead>
	<tbody>
	
	</tbody>
	</table>

	</div>
</div>
<h2>Import Products</h2>

<%= form_tag import_accounts_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>
</div>
</div>





